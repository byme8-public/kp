<UserControl x:Class="kp.Views.MainView" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:ns0="clr-namespace:kp.ViewModels" xmlns:ns1="clr-namespace:WpfToolkit.ViewModels.Injection;assembly=WPFToolkit" xmlns:ns2="clr-namespace:kp.Views" xmlns:ns3="clr-namespace:kp.ViewModels.UserRoles" xmlns:ns4="clr-namespace:kp.Views.UserRoles" xmlns:ns5="clr-namespace:kp.ViewModels.Clients" xmlns:ns6="clr-namespace:kp.Views.Clients" xmlns:ns7="clr-namespace:kp.ViewModels.PaymentsKinds" xmlns:ns8="clr-namespace:kp.Views.PaymentKinds" xmlns:ns9="clr-namespace:Dragablz;assembly=Dragablz" xmlns:ns10="clr-namespace:AmmySidekick;assembly=AmmySidekick" ns10:Ammy.Register="/kp;component/Views/MainView.g.xaml">
